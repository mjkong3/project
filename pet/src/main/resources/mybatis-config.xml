        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="customerDto" type="com.example.pet.dto.CustomerDto"/>
        <typeAlias alias="categoryDto" type="com.example.pet.dto.CategoryDto"/>
        <typeAlias alias="itemDto" type="com.example.pet.dto.ItemDto"/>
