        <typeAlias alias="reviewDto" type="com.example.pet.review.dto.ReviewDto" />
        <typeAlias alias="adminItemDto" type="com.example.admin.dto.AdminItemDto"/>
        <typeAlias alias="adminCustomerDto" type="com.example.admin.dto.AdminCustomerDto"/>
        <typeAlias alias="adminOrderListDto" type="com.example.admin.dto.AdminOrderListDto"/>
        <typeAlias alias="adminCategoryDto" type="com.example.admin.dto.AdminCategoryDto"/>
        <typeAlias alias="orderListDto" type="com.example.pet.dto.OrderListDto" />
        <typeAlias alias="cartDto" type="com.example.pet.dto.CartDto" />
        <typeAlias alias="cartDetailDto" type="com.example.pet.dto.CartDetailDto" />
        <typeAlias alias="cartItemDto" type="com.example.pet.dto.CartItemDto" />
        <typeAlias alias="orderDto" type="com.example.pet.dto.OrderDto"/>
        <typeAlias alias="orderDetailDto" type="com.example.pet.dto.OrderDetailDto"/>
        <typeAlias alias="orderHistDto" type="com.example.pet.dto.OrderHistDto"/>
